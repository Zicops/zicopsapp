{
  "extends": "next/core-web-vitals",
  "plugins": [
    "spellcheck"
  ],
  "rules": {
    "spellcheck/spell-checker": [
      1,
      {
        "comments": true,
        "strings": true,
        "identifiers": true,
        "templates": true,
        "lang": "en_US",
        "skipWords": [
          "zicops",
          "Msg",
          "Dropdown",
          "webkit",
          "scrollbar",
          "ffffff",
          "svg",
          "ul",
          "li"
        ]
      }
    ]
  }
}